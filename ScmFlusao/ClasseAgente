public class Agente {
    private int id; 
    private String nome; 

    public Agente(int id, String nome) {
        this.id = id;
        this.nome = nome;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getNome() {
        return nome;
    }

    public void setNome(String nome) {
        this.nome = nome;
    }

    public void cadastrarAgente() {
        System.out.println("Agent " + this.nome + " (ID: " + this.id + ") registrado.");
    }

    public void selecionarAgente() {
        System.out.println("Agent " + this.nome + " (ID: " + this.id + ") selecionado.");
    }
}
